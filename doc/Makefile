doc: pdfs
	pdflatex m-etherlab.tex

pdfs:
	$(MAKE) -C images pdfs

clean:
	rm -f \
		*.aux \
		*.lof \
		*.log \
		*.maf \
		*.mtc \
		*.mtc1 \
		*.out \
		*.pdf \
		*.toc

.PHONY: doc

