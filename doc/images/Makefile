FIGS := \
	c-api.fig \
	etl-arch.fig

PDFS = $(FIGS:.fig=.pdf)

all: $(PDFS)

%.pdf: %.fig
	fig2dev -L pdf -z A4 -p xxx -c $< $@	

clean:
	@rm -rv $(PDFS)

