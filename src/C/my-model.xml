<?xml version="1.0"?>
<modeldescription modelname="my-model">
  <description>My HandCoded C Model</description>
  <version>11</version>
  <properties max_overruns="5" stacksize="2048" sample_decimation="1" buffer_time="10"/>

  <!-- Enter the base tick in microseconds and the number of sample times -->
  <task basetick="1000">
      <subtask decimation="1"/>
      <subtask decimation="100"/>
  </task>

  <data>
    <signal name="rootpressure" cvar="var1" datatype="double_T" orientation='vector' elements="10" sampleTimeIndex="0"/>
    <subsystem name="meters">
    <subsystem name="meter1">
      <signal name="pressure" cvar="var2" datatype="double_T" rnum="10" sampleTimeIndex="0">
        <alias>X</alias>
        <meta hide="0"/>
      </signal>
      <parameter name="scalar_param" cvar="scalar_param" datatype="double_T"
         value="4"/>
    </subsystem>
    </subsystem>
  </data>some more junk
</modeldescription>
