<?xml version="1.0"?>
<model name="my-model">
  <description>My HandCoded C Model</description>
  <version>11</version>
  <parameters max_overruns="5" stacksize="2048" photo_decimation="1" buffer_time="10"/>

  <!-- Enter the base tick in microseconds for the main task. If you
       have subtasks, add them inside <task> with the according decimation -->
  <task basetick="1000">
      <subtask decimation="100"/>
  </task>

    <!-- signal name="rootpressure" cvar="var1" datatype="double_T" orientation='vector' elements="10" sampleTimeIndex="0"/ -->

  <subsystem name="subsysname">
    <dim value="3"/>
    <signal name="pressure" cvar="var2" datatype="double_T"/>
    <parameter name="scalar_param" cvar="scalar_param" datatype="double_T">
     <value>4</value>
    </parameter>
  </subsystem>

  <data>
    <subsystem name="meters">
    <subsystem name="meter1">
      <dim value="3"/>
      <signal name="pressure" cvar="var2" datatype="double_T" 
               sampleTimeIndex="1" alias="X">
       <dim value="10"/>
       <dim value="20"/>
       <dim value="30"/>
       <meta>hide="0"</meta>
      </signal>
      <parameter name="scalar_param" cvar="scalar_param" datatype="double_T">
       <value>4</value>
      </parameter>
    </subsystem>
    </subsystem>
    <cstruct name="cstruct">
     <dim value="4"/>
    </cstruct>
    <reference name="reference" value="subsysname">
     <dim value="4"/>
    </reference>
  </data>
</model>
