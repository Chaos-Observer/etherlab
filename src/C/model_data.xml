<modeldescription modelname="my-model">
    <description>My HandCoded C Model</description>

    <sampleTimes tick="1000" count="2">
      <sampletime index="0" decimation="1"/>
      <sampletime index="1" decimation="100"/>
    </sampleTimes>

    <signals>
      <signal id="var1" datatype="double" dimension="10" sampleTimeIndex="0">
        <path>outputs/meter</path>
        <alias>X</alias>
        <meta hide="0"/>
      </signal>
      <signal id="var2" datatype="double" sampleTimeIndex="0">
        <alias>X</alias>
        <meta hide="0"/>
      </signal>
    </signals>

    <parameters>
      <parameter id="param1" datatype="double" dimension="10" value="0.3">
        <path>Input/Gain1</path>
        <meta ro="1"/>
        <value>0.3</value>
      </parameter>
    </parameters>

</modeldescription>
