"\n% Find the product code from the available models\nmodels = { ...\n% Model "
"   Vendor Id            Diagnostic  DefaultLayout\n    '2004' hex2dec('07d430"
"52')  0           0 ; ...\n    '2008' hex2dec('07d83052')  0           1 ; .."
".\n    '2032' hex2dec('07f03052')  1           0 ; ...\n    '2602' hex2dec('0"
"a2a3052')  0           1 ; ...\n    '2612' hex2dec('0a343052')  0           1"
" ; ...\n    '2622' hex2dec('0a3e3052')  0           1 ; ...\n    };\nproduct_"
"code = 0;\ndiagnostic = 0;\ndefault_layout = 0;\nif size(model,2) >= 6\n    i"
"dx = find(strcmp(models(:,1),model(3:6)));\n    if ~isempty(idx)\n        pro"
"duct_code = cell2mat(models(idx,2));\n        diagnostic   = cell2mat(models("
"idx,3));\n        default_layout   = cell2mat(models(idx,4));\n    end\nend\n"
"if ~product_code\n    errordlg([gcb ': Slave model \"' model '\" is not imple"
"mented'])\nend\n\nvendor = 2;\n\n% Number of channels is given by the 6'th ch"
"aracter of model string\nchannels = str2double(model(6));\nif default_layout "
"== 1 || ~isempty(findstr(model,'v2'))\n    layout = 1;\n    % Create a matrix"
" \n    % pdo_entry  = [ 0x7000 1 1;\n    %                0x7010 1 1;\n    % "
"               .... \n    %               ]\n    % with (channel) number of r"
"ows\n    pdo_entry = ones(channels,3);\n    pdo_entry(:,1) = [hex2dec('7000')"
":16:hex2dec('7000')+(channels-1)*16]';\n    \n    if diagnostic\n        stat"
"us_pdo = ones(channels,3);\n        status_pdo(:,1) = [hex2dec('6000'):16:hex"
"2dec('6000')+(channels-1)*16]';\n        pdo_entry = [pdo_entry; status_pdo];"
"\n    end\nelse\n    layout = 0;\n    % Create a matrix \n    % pdo_entry  = "
"[ 0x3001 1 1;\n    %                0x3001 2 1;\n    %                .... \n"
"    %               ]\n    % with (channel) number of rows\n    pdo_entry = r"
"epmat([hex2dec('3001') 1 1],channels,1);\n    pdo_entry(:,2) = [1:channels]';"
"\n    \n    if diagnostic\n        status_pdo = repmat([hex2dec('3101') 1 1],"
"channels,1);\n        status_pdo(:,2) = [1:channels]';\n        pdo_entry = ["
"pdo_entry; status_pdo];\n    end\nend\n\npdo_info = repmat([1 hex2dec('1600')"
" 0 1], channels, 1);\npdo_info(:,2) = pdo_info(:,2) + [0:channels-1]';\nif di"
"agnostic\n    status_pdo_info = repmat([0 hex2dec('1A00') 0 1], channels, 1);"
"\n    status_pdo_info(:,2) = status_pdo_info(:,2) + [0:channels-1]';\n    pdo"
"_info = [pdo_info; status_pdo_info];\nend\npdo_info(:,3) = [0:size(pdo_info,1"
")-1]';\n\nsdo_config = [];\n\n% The first column of input_pdo_map are the 0-b"
"ased\n% indices of pdo_info where the first column == 1\n% The second column "
"of input_pdo_map are all 0\ninput_pdo_map = zeros(channels,2);\ninput_pdo_map"
"(:,1) = find(pdo_info(:,1)' == 1)-1;\n\nif status && diagnostic\n    % The fi"
"rst column of output_pdo_map are the 0-based\n    % indices of pdo_info where"
" the first column == 0\n    % The second column of output_pdo_map are all 0\n"
"    output_pdo_map = zeros(channels,2);\n    output_pdo_map(:,1) = find(pdo_i"
"nfo(:,1)' == 0)-1;\nelse\n    output_pdo_map = [];\nend\n\noutput = [];\nif s"
"trcmp(input_type,'Vector Input')\n    input = struct('pdo_map', input_pdo_map"
");\n    if size(output_pdo_map)\n        output = struct('pdo_map', output_pd"
"o_map);\n    end\nelseif strcmp(input_type, 'Separate Inputs')\n    % Distrib"
"ute each row of input_pdo_map to input(x).pdo_map\n    input  = struct('pdo_m"
"ap', mat2cell( input_pdo_map,ones(1,channels),2));\n    if size(output_pdo_ma"
"p)\n        output = struct('pdo_map', mat2cell(output_pdo_map,ones(1,channel"
"s),2));\n    end\nelse\n    errordlg([gcb ': unknown input type: ' output_typ"
"e]);\nend\n\n"
	  MaskDisplay		  "fprintf('%s\\nM:%s Idx:%s',...\n    model,m"
"asterStr,positionStr)\n\n"
	  MaskIconFrame		  on
	  MaskIconOpaque	  on
	  MaskIconRotate	  "none"
	  MaskIconUnits		  "autoscale"
	  MaskValueString	  "0|1|EL2004v2|Vector Input|off|0"
	  MaskTabNameString	  ",,,,,"
	}
