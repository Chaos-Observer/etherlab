	  MaskToolTipString	  "on,on,on,on,on,on"
	  MaskVarAliasString	  ",,,,,"
	  MaskVariables		  "master=@1;index=@2;model=&3;input_type=@4;s"
"cale=@5;tsample=@6;"
	  MaskInitialization	  "try\n    [master domain alias position mast"
