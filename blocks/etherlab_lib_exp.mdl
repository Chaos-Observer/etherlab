"strcmp(input_type,'Vector Input')\n        % Only one output, with all rows o"
"f pdo_map\n        PdoEntryMap = generateIO(device, 'GroupType', 'All',...\n "
"           'SelectPdo', PdoIndexList);\n        IOSpec = cell2struct(PdoEntry"
"Map,'PdoEntry',1);\n    elseif strcmp(input_type, 'Separate Inputs')\n       "
" % Have (channel) number of outputs, each containing one row of pdo_map\n    "
"    PdoEntryMap = generateIO(device);\n        IOSpec = cell2struct(PdoEntryM"
"ap,'PdoEntry',1);\n    else\n        errordlg([gcb ': unknown input type: ' i"
"nput_type]);\n    end\nend\n"
	  MaskDisplay		  "fprintf('%s\\nM:%s Idx:%s',...\n    model,m"
"asterStr,positionStr)\n\n"
