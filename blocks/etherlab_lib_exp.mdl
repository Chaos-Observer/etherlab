"\n            else\n                output.filter = 2*pi./tau;\n             "
"   output.filter_name = 'Omega';\n            end\n        end\n    end\n\n  "
"  if status\n        output(2).pdo_map = [0:channels-1; zeros(1,channels)]';"
"\n    end\n\nelse\n    % Separate outputs chosen\n    % Distribute each row o"
"f output_pdo_map to output(x).pdo_map\n    output = struct('pdo_map', mat2cel"
"l(output_pdo_map,ones(1,channels),2));\n\n    if dtype ~= 1\n        for i = "
"1:channels\n            output(i).pdo_full_scale = 2^15;\n        end\n      "
"  \n        if dtype == 3\n            if ~isempty(scale)\n                fo"
"r i = 1:channels\n                    % Choose scale(1) for every output if s"
"cale\n                    % is a scalar\n                    output(i).gain ="
