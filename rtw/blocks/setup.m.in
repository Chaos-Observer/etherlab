function setup()

if ~length(strfind(path,pwd))
    disp(['Adding ' pwd ' to $MATLABPATH']);
    addpath(pwd);
end

disp(['Precompiling functions in ' pwd]);
mex taskinfo.c
mex world_time.c
mex raise.c
mex etherlab_in.c
mex etherlab_out.c

run EtherCAT/setup.m

if @EXPERIMENTAL@
  run AddiData/setup.m
  run Hilscher/setup.m
end

return
